\if@onesideprint
	\ClassWarning{cquthesis}{Printing under oneside mode.}{}
\else
	\if@twosideprint
		\geometry{twoside}
		\ClassWarning{cquthesis}{Printing under twoside mode.}{}
	\fi
\fi
