%    \begin{macrocode}
\RequirePackage{geometry}
\geometry{
	a4paper, ignoreall, nomarginpar,
	inner=25mm, outer=25mm, top=30mm, bottom=28mm,
	bindingoffset=10mm, head=16mm, headsep=11mm, foot=12mm, footnotesep=5mm}
